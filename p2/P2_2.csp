;Authors:Courtney Laux, Angela(Haowen) Zhou

LDS# $E00
.EQU	GET_NUM,$E01
.EQU	PUT_NUM,$E00

	JSR GET_NUM
	STA N
	CMA# 1
	JEQ PRINT1
	CMA# 2
	JEQ PRINT1
	CMA# 2
	JGT RECURSION
	
PRINT1:	LDA FIBON
	JSR PUT_NUM
	HLT

RECURSION:	LDA FNM1
	ADA FNM2
	STA FN
;	JSR PUT_NUM
	LDA FNM1
	STA FNM2
	LDA FN
	STA FNM1
	INC I
	LDA I
	CMA N
	JLE RECURSION

	LDA FN
	JSR PUT_NUM
	
	HLT
	


N:	.WORD	0
FN:	.WORD	0
FNM1:	.WORD	1
FNM2:	.WORD	1
I:	.WORD	3
FIBON:	.WORD	1